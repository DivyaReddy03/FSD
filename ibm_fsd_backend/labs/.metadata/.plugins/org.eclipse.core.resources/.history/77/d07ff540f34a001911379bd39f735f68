package com.java.examples.RAPI;

public class Calc {
	Calc()
	{
		
	}

	public Integer add(Integer a, Integer b)
	{
		return a+b;
		
	}
	public Integer diff(Integer a , Integer b)
	{
		return a-b;
	}
	public Integer mul(Integer a, Integer b)
	{
		return a*b;
	}
	public Integer div(Integer a, Integer b)
	{
		return a/b;
	}
	
}
